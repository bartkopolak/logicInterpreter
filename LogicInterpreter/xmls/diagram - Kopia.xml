<?xml version="1.0" encoding="utf-8"?>
<diagram name="xor2">
<blocks>
	<block>
		<id>nand2</id>
		<src>nand.xml</src>
	</block>
	<block>
		<id>and2</id>
		<src>and.xml</src>
	</block>
	<block>
		<id>or2</id>
		<src>or.xml</src>
	</block>	
</blocks>

<outputs>
	<output>X</output>
	<output>X1</output>
	<output>X2</output>
	<output>X3</output>
	<output>X4</output>
	<output>X5</output>
	<output>X6</output>
	<output>X7</output>
	<output>X8</output>
	<output>X9</output>
	<output>X10</output>
	<output>X11</output>
	<output>X12</output>
	<output>X13</output>
	<output>X14</output>
	<output>X15</output>
</outputs>

<inputs>
	<input>
		<name>A</name>
		<links>
			<link to="nand2.A" />
			<link to="or2.B" />
		</links>
	</input>
	<input>
		<name>B</name>
		<links>
			<link to="nand2.B"/>
			<link to="or2.A" />
		</links>
	</input>
</inputs>

<wires>
	<wire>
		<id>nand2.x_To_and2.a</id>
		<from>nand2.X</from>
		<to>and2.A</to>
	</wire>
	<wire>
		<id>or2.x_To_and2.b</id>
		<from>or2.X</from>
		<to>and2.B</to>
	</wire>
	<wire>
		<id>and2.x_To_output</id>
		<from>and2.X</from>
		<to>outputs.X</to>
	</wire>
</wires>

</diagram>

<!--
blocks - pojedyncze bloki wystepujace w diagramie
outputs - wyjscia
inputs - wejscia + polaczenia
wires - polaczenia 
	from output
	to input
-->