<?xml version="1.0" encoding="utf-8"?>
<diagram name="xor2">
<blocks>
	<block>
		<id>licznik</id>
		<src>licznikasync_block.xml</src>
	</block>
	<block>
		<id>binto7sd</id>
		<src>binto7sd.xml</src>
	</block>	
</blocks>

<outputs>
	<output>HEX_A</output>
	<output>HEX_B</output>
	<output>HEX_C</output>
	<output>HEX_D</output>
	<output>HEX_E</output>
	<output>HEX_F</output>
	<output>HEX_G</output>
</outputs>

<inputs>
	<input>
		<name>VCC</name>
		<links>
			<link to="licznik.VCC" />
		</links>
	</input>
	<input>
		<name>CLK</name>
		<links>
			<link to="licznik.CLK"/>
		</links>
	</input>
</inputs>

<wires>
	<wire>
		<id>w1</id>
		<from>licznik.D3</from>
		<to>binto7sd.A</to>
	</wire>
	<wire>
		<id>w2</id>
		<from>licznik.D2</from>
		<to>binto7sd.B</to>
	</wire>
	<wire>
		<id>w3</id>
		<from>licznik.D1</from>
		<to>binto7sd.C</to>
	</wire>
	<wire>
		<id>w4</id>
		<from>licznik.D0</from>
		<to>binto7sd.D</to>
	</wire>
	<wire>
		<id>w5</id>
		<from>binto7sd.OA</from>
		<to>outputs.HEX_A</to>
	</wire>
	<wire>
		<id>w6</id>
		<from>binto7sd.OB</from>
		<to>outputs.HEX_B</to>
	</wire>
	<wire>
		<id>w7</id>
		<from>binto7sd.OC</from>
		<to>outputs.HEX_C</to>
	</wire>
	<wire>
		<id>w8</id>
		<from>binto7sd.OD</from>
		<to>outputs.HEX_D</to>
	</wire>
	<wire>
		<id>w9</id>
		<from>binto7sd.OE</from>
		<to>outputs.HEX_E</to>
	</wire>
	<wire>
		<id>w10</id>
		<from>binto7sd.OF</from>
		<to>outputs.HEX_F</to>
	</wire>
	<wire>
		<id>w11</id>
		<from>binto7sd.OG</from>
		<to>outputs.HEX_G</to>
	</wire>
</wires>

</diagram>

<!--
blocks - pojedyncze bloki wystepujace w diagramie
outputs - wyjscia
inputs - wejscia + polaczenia
wires - polaczenia 
	from output
	to input
-->