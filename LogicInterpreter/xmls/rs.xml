<?xml version="1.0" encoding="utf-8"?>
<diagram name="ns flip flop">
<blocks>
	<block>
		<id>nand1</id>
		<src>nand.xml</src>
	</block>	
	<block>
		<id>nand2</id>
		<src>nand.xml</src>
	</block>
</blocks>

<outputs>
	<output>Q</output>
	<output>NQ</output>
</outputs>

<inputs>
	<input>
		<name>R</name>
		<links>
			<link to="nand2.B" />
		</links>
	</input>
	<input>
		<name>S</name>
		<links>
			<link to="nand1.A"/>
		</links>
	</input>
</inputs>

<wires>
	<wire>
		<id>w1</id>
		<from>nand1.X</from>
		<to>outputs.Q</to>
		<to>nand2.A</to>

	</wire>
	<wire>
		<id>w2</id>
		<from>nand2.X</from>
		<to>outputs.NQ</to>
		<to>nand1.B</to>

	</wire>
</wires>

</diagram>
